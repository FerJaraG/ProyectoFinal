<header id="gtco-header" class="gtco-cover gtco-cover-sm" role="banner">
		<div class="overlay"></div>
		<div class="gtco-container">
			<div class="row">
				<div class="col-md-12 col-md-offset-0 text-center">
					<div class="row row-mt-15em">

						<div class="col-md-12 mt-text animate-box" data-animate-effect="fadeInUp">
							<h1></h1>	
						</div>
						
					</div>
					
				</div>
			</div>
		</div>
	</header>
	<p><%= notice %></p>
    <p><%= alert %></p>

	<div class="gtco-section">
		<div class="gtco-container">
			<div class="row">
				<div class="col-md-8 col-md-offset-2 text-center gtco-heading">
					<h2>Administrar Camping</h2>
						<ul class="pricing-info">
                            <%= link_to 'Mis Sitios', camping_campsites_path(@camping), class: "btn btn-dark"%>
                            <%= link_to 'Mis Planes', camping_plans_path(@camping), class: "btn btn-dark"%>
                            <%= link_to 'Mis Reservas', camping_bookings_path(@camping), class: "btn btn-dark"%>
							<%= link_to 'Editar Camping', edit_camping_path(@camping), class: "btn btn-dark"%>
                        </ul>
				</div>
			</div>

			<div class="row">
				
			</div>
		</div>
	</div>
    
